{
  "kind": "collectionType",
  "collectionName": "schedules",
  "info": {
    "singularName": "schedule",
    "pluralName": "schedules",
    "displayName": "Schedule"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tanggal": {
      "type": "date"
    },
    "jam_mulai": {
      "type": "time"
    },
    "jam_selesai": {
      "type": "time"
    },
    "isBooked": {
      "type": "boolean",
      "default": false
    },
    "statusJadwal": {
      "type": "enumeration",
      "enum": [
        "Scheduled ",
        "Completed",
        "Cancelled"
      ]
    },
    "bookedBy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "schedule"
    },
    "phoneNumber": {
      "type": "string"
    }
  }
}
